          <span>High Five</span>
        </a>
      </div>
      <nav>
        <ul class="nav-list">
          <% categories.forEach(category => { %>
            <li class="nav-item">
              <a class="nav-link" href="/categoria/<%= category.CategoryID %> "><%= category.CategoryName %> </a>
